{
    "name": "aotr/laravel-tasker",
    "description": "Help to Laravel automate things.",
    "keywords": ["tasker", "laravel", "automate", "console", "cli"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Animesh Chakraborty",
            "email": "animesh.aotr@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel-zero/phar-updater": "^1.2",
        "nunomaduro/termwind": "^1.14"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3"
    },
    "autoload": {
        "psr-4": {
            "Aotr\\Tasker\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "suggest": {
        "laravel-zero/framework": "Required for using the package with Laravel Zero."
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["tasker"]
}
